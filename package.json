{
  "name": "@mocks-server/admin-api-client",
  "version": "1.0.0",
  "description": "Client of @mocks-server/plugin-admin-api",
  "keywords": [
    "mocks-server-plugin",
    "administration",
    "admin",
    "api",
    "rest",
    "client"
  ],
  "author": "Javier Brea",
  "license": "MIT",
  "repository": "https://github.com/mocks-server/admin-api-client",
  "homepage": "https://www.mocks-server.org",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "main": "dist/index.umd.js",
  "module": "dist/index.esm.js",
  "scripts": {
    "build": "rollup --config",
    "lint": "eslint index.js test src jest.config.js test-e2e/browser/react-app/src test-e2e/browser/cypress",
    "lint-staged": "lint-staged",
    "test": "jest",
    "coveralls": "cat ./coverage/lcov.info | coveralls"
  },
  "dependencies": {
    "@data-provider/axios": "^1.5.0",
    "@data-provider/core": "^1.7.0",
    "@mocks-server/admin-api-paths": "^1.0.1"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.7.7",
    "@rollup/plugin-commonjs": "^11.0.0",
    "@rollup/plugin-node-resolve": "^6.0.0",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "babel-polyfill": "^6.26.0",
    "coveralls": "^3.0.7",
    "eslint": "6.6.0",
    "eslint-config-prettier": "6.5.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.17.0",
    "husky": "3.0.9",
    "jest": "24.9.0",
    "lint-staged": "^9.4.2",
    "prettier": "^1.19.1",
    "rollup": "^1.27.14",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-uglify": "^6.0.4",
    "sinon": "^8.0.1"
  },
  "lint-staged": {
    "test/**/*.js": "eslint",
    "src/**/*.js": "eslint",
    "index.js": "eslint",
    "jest.config.js": "eslint",
    "test-e2e/browser/react-app/src/**/*.js": "eslint",
    "test-e2e/browser/cypress/**/*.js": "eslint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint-staged"
    }
  },
  "engines": {
    "node": ">8.0.0",
    "npm": ">6.0.0"
  }
}

  },
  "devDependencies": {
    "@mocks-server/core": "2.0.0-beta.1",
    "cross-fetch": "3.0.6",
    "eslint": "7.12.1",
    "eslint-plugin-no-only-tests": "2.4.0",
    "eslint-config-prettier": "6.15.0",
    "eslint-plugin-prettier": "3.1.4",
    "husky": "4.3.0",
    "jest": "26.6.3",
    "lint-staged": "10.5.0",
    "prettier": "2.1.2",
    "sinon": "9.2.0",
    "wait-on": "5.2.1"
  },
  "lint-staged": {
    "src/**/*.js": "eslint",
    "test/**/*.js": "eslint",
    "*.js": "eslint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint-staged"
    }
  },
  "engines": {
    "node": "12.x || 14.x || 15.x"
  }

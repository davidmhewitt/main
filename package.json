{
  "name": "@mocks-server/plugin-inquirer-cli",
  "version": "2.0.0",
  "description": "Mocks server plugin providing an interactive CLI",
  "keywords": [
    "mocks-server-plugin",
    "interactive",
    "cli",
    "inquirer",
    "settings",
    "administration",
    "options",
    "development"
  ],
  "author": "Javier Brea",
  "license": "Apache-2.0",
  "repository": "https://github.com/mocks-server/plugin-inquirer-cli",
  "homepage": "https://www.mocks-server.org",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "src",
    "index.js"
  ],
  "main": "index.js",
  "scripts": {
    "lint": "eslint *.js src test",
    "lint-staged": "lint-staged",
    "test": "jest",
    "test:e2e": "jest --config=jest.e2e.config.js --runInBand",
    "test:unit": "npm run test"
  },
  "peerDependencies": {
    "@mocks-server/core": "2.x"
  },
  "dependencies": {
    "chalk": "4.1.0",
    "inquirer": "7.3.3",
    "inquirer-autocomplete-prompt": "1.3.0",
    "lodash": "4.17.20",
    "node-emoji": "1.10.0"
  },
  "devDependencies": {
    "@mocks-server/core": "2.1.0",
    "cross-fetch": "3.0.6",
    "cross-spawn": "7.0.3",
    "eslint": "7.23.0",
    "eslint-plugin-no-only-tests": "2.4.0",
    "eslint-config-prettier": "7.2.0",
    "eslint-plugin-prettier": "3.4.0",
    "fs-extra": "9.0.1",
    "husky": "4.3.6",
    "is-promise": "4.0.0",
    "jest": "26.6.3",
    "lint-staged": "10.5.4",
    "prettier": "2.2.1",
    "request": "2.88.2",
    "request-promise": "4.2.6",
    "sinon": "9.2.4",
    "strip-ansi": "6.0.0",
    "tree-kill": "1.2.2",
    "wait-on": "5.2.1"
  },
  "lint-staged": {
    "src/**/*.js": "eslint",
    "test/**/*.js": "eslint",
    "*.js": "eslint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint-staged"
    }
  },
  "engines": {
    "node": "12.x || 14.x || 15.x"
  }
}

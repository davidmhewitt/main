{
  "name": "@mocks-server/plugin-admin-api",
  "version": "1.4.7",
  "description": "Plugin for Mocks Server. Provides a REST API for administrating settings, fixtures and behaviors",
  "keywords": [
    "mocks-server-plugin",
    "administration",
    "admin",
    "api",
    "rest",
    "testing",
    "development"
  ],
  "author": "Javier Brea",
  "license": "Apache-2.0",
  "repository": "https://github.com/mocks-server/plugin-admin-api",
  "homepage": "https://www.mocks-server.org",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "src",
    "index.js"
  ],
  "main": "index.js",
  "scripts": {
    "lint": "eslint *.js src test",
    "lint-staged": "lint-staged",
    "test": "jest",
    "test:e2e": "jest --config=jest.e2e.config.js --runInBand",
    "test:unit": "npm run test"
  },
  "peerDependencies": {
    "@mocks-server/core": ">=1.3.0"
  },
  "dependencies": {
    "@mocks-server/admin-api-paths": "1.0.8",
    "@hapi/boom": "9.1.0",
    "express": "4.17.1"
  },
  "devDependencies": {
    "@mocks-server/core": "1.5.1",
    "eslint": "7.12.1",
    "eslint-config-prettier": "6.15.0",
    "eslint-plugin-prettier": "3.1.4",
    "fs-extra": "9.0.1",
    "husky": "4.3.0",
    "is-promise": "4.0.0",
    "jest": "26.6.1",
    "lint-staged": "10.5.0",
    "prettier": "2.1.2",
    "request": "2.88.2",
    "request-promise": "4.2.6",
    "sinon": "9.2.0",
    "strip-ansi": "6.0.0",
    "tree-kill-sync": "1.0.0"
  },
  "lint-staged": {
    "src/**/*.js": "eslint",
    "test/**/*.js": "eslint",
    "*.js": "eslint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint-staged"
    }
  },
  "engines": {
    "node": "12.x || 14.x || 15.x"
  }
}

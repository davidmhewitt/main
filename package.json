{
  "name": "@mocks-server/admin-api-client",
  "version": "2.0.4",
  "description": "Client of @mocks-server/plugin-admin-api",
  "keywords": [
    "mocks-server-plugin",
    "administration",
    "admin",
    "api",
    "rest",
    "client"
  ],
  "author": "Javier Brea",
  "license": "MIT",
  "repository": "https://github.com/mocks-server/admin-api-client",
  "homepage": "https://www.mocks-server.org",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "main": "dist/index.umd.js",
  "module": "dist/index.esm.js",
  "scripts": {
    "build": "rollup --config",
    "lint": "eslint *.js test src jest.config.js",
    "lint-staged": "lint-staged",
    "mocks:install": "cd mocks && npm i",
    "mocks:ci": "cd mocks && npm run mocks:ci",
    "test": "jest",
    "test:coverage": "npm run mocks:install && start-server-and-test mocks:ci tcp:3200 test",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "test:ci": "npm run test:coverage"
  },
  "dependencies": {
    "cross-fetch": "3.0.6",
    "@mocks-server/admin-api-paths": "1.0.8"
  },
  "devDependencies": {
    "@babel/preset-env": "7.12.1",
    "@rollup/plugin-babel": "5.2.1",
    "@rollup/plugin-commonjs": "15.1.0",
    "@rollup/plugin-node-resolve": "9.0.0",
    "babel-eslint": "10.1.0",
    "babel-jest": "26.6.1",
    "babel-polyfill": "6.26.0",
    "coveralls": "3.0.9",
    "eslint": "7.12.1",
    "eslint-config-prettier": "6.15.0",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-react": "7.21.5",
    "husky": "4.3.0",
    "jest": "26.6.1",
    "lint-staged": "10.5.0",
    "prettier": "2.1.2",
    "rollup": "2.32.1",
    "rollup-plugin-terser": "7.0.2",
    "sinon": "9.2.0",
    "start-server-and-test": "1.11.5"
  },
  "lint-staged": {
    "test/**/*.js": "eslint",
    "src/**/*.js": "eslint",
    "*.js": "eslint",
    "jest.config.js": "eslint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint-staged"
    }
  },
  "engines": {
    "node": "12.x || 14.x"
  }
}

|<h3>✏️ Changes impact</h3>
{{#if affected.length}}
<ul>
{{#if applications.length}}
<li>{{{applications.length}}} {{#if applicationsArePlural}}applications{{else}}application{{/if}}: {{#each applications}}{{#if @index}}, {{/if}}<code>{{{this}}}</code>{{/each}}</li>
{{/if}}
{{#if libraries.length}}
<li>{{{libraries.length}}} {{#if librariesArePlural}}libraries{{else}}library{{/if}}: {{#each libraries}}{{#if @index}}, {{/if}}<code>{{{this}}}</code>{{/each}}</li>
{{/if}}
{{#if test.length}}
<li>{{{test.length}}} test: {{#each test}}{{#if @index}}, {{/if}}<code>{{{this}}}</code>{{/each}}</li>
{{/if}}
</ul>
<strong>Total: {{affected.length}} affected projects</strong> 
{{else}}
<strong>No affected projects found</strong>
{{/if}}
